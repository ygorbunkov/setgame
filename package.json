  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.1.1"
  },
  "scripts": {
    "format:write": "yarn run prettier --write --ignore-unknown \"**/*\"",
    "format:check": "yarn run prettier --check --ignore-unknown \"**/*\"",
    "eslint:check": "yarn eslint \"src/**\"",
    "eslint:fix": "yarn eslint --fix \"src/**\"",
    "ts:check": "tsc --noEmit --project ./tsconfig.json",
    "clean": "rm -rf dist/*",
    "start:dev": "webpack serve --hot --mode development",
    "start:prod": "webpack --mode production && NODE_ENV=production ts-node ./src/server/index.ts"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@hot-loader/react-dom": "17.0.1",
    "@testing-library/react": "^11.2.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.27",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.0",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
